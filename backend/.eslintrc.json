{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes" : ["error", "single"],
        "indent": ["error", 4],
        "linebreak-style" : [0],
        "semi": [2, "always"],
        "prefer-destructuring" : ["warn", 2]

        // "prettier/prettier": [
